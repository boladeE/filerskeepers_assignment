          "parameters": [
            {
              "name": "category",
              "in": "query",
              "required": false,
              "schema": {
                "anyOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "null"
                  }
                ],
                "description": "Filter by category",
                "title": "Category"
              },
              "description": "Filter by category"
            },
            {
              "name": "min_price",
              "in": "query",
              "required": false,
              "schema": {
                "anyOf": [
                  {
                    "type": "number"
